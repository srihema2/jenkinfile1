pipeline{
    agent any
    stages{
        stage('compile'){
            steps{
                echo "compiling the script"
            }
        }
        stage('build'){
            steps{
                echo "building the script"
            }
        }
        stage('test'){
            steps{
                echo "testing the script"
            }
        }
        stage('deploy'){
            steps{
                echo "deploying the script"
            }
        }
        stage('release'){
            steps{
                echo "releasing the script"
            }
        }
    }
}
